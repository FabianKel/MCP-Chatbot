[
  {
    "name": "transfermarkt",
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "tsx", "src/servers/transfermarkt-server/transfermarkt-server.ts"],
    "cwd": "."
  },

  {
    "name": "filesystem",
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-filesystem", "./src/fileSystem_playground"]
  },

  {
    "name": "git",
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-github"],
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "GITHUB_PERSONAL_ACCESS_TOKEN"
    }
  },
  {
    "name": "kitchen",
    "type": "stdio",
    "command": "node",
    "args": ["src/servers/kitchen-server/src/mcp-server.js"]
  },
  {
    "name": "remote-get-random-number",
    "type": "url",
    "url": "https://google.com"
  }

]
